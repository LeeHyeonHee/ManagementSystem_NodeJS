<body>






    <div id="container" class="container">

        <h1 id="container_title"><span>비밀번호 변경</span></h1>




        <div id="mb_login" class="mbskin">

            <h1>비밀번호 변경   </h1>




            <form name="flogin" action="/changePW/<%= id %>" onsubmit="return flogin_submit(this);" method="post" id="flogin">


                <div id="login_frm">

                    <label for="login_id" class="sound_only">아이디</label>

                    <p><%= id %></p>

                    <label for="login_pw" class="sound_only">비밀번호</label>

                    <input type="password" name="pw" id="login_pw" placeholder="비밀번호" required=""
                        class="frm_input required" maxlength="20">

                        <label for="login_pw" class="sound_only">비밀번호 확인</label>
                        
                    <input type="password" name="pw2" id="login_pw2" placeholder="비밀번호확인" required=""
                    class="frm_input required" maxlength="20">

            
                    <input type="submit" value="변경" class="btn_submit">

                </div>






        </div>
        </form>